  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:api\" \"npm:dev:web\"",
    "dev:web": "vite",
    "dev:api": "nodemon --watch server --ext js,mjs --exec node server/index.js",
    "build": "tsc -b && vite build",
